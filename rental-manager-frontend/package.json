  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.(js|jsx|ts|tsx)",
      "**/*.(test|spec).(js|jsx|ts|tsx)",
      "**/tests/**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "testTimeout": 600000,
    "maxWorkers": 1,
    "verbose": true,
    "bail": false
  }
